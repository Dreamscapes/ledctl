{
  "name": "ledctl",
  "description": "Control your LEDs from JavaScript or using terminal",
  "version": "0.2.1",
  "homepage": "https://github.com/Dreamscapes/ledctl",
  "author": {
    "name": "Robert Rossmann",
    "email": "rr.rossmann@me.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Dreamscapes/ledctl.git"
  },
  "main": "index.js",
  "bin": "bin/ledctl",
  "engines": {
    "node": ">= 0.10.0"
  },
  "os": [
    "linux"
  ],
  "scripts": {
    "test": "make lint test"
  },
  "dependencies": {
    "async": "^0.9.0",
    "lodash": "^2.4.1",
    "nomnom": "^1.8.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "jaguarjs-jsdoc": "git://github.com/davidshimjs/jaguarjs-jsdoc.git",
    "jsdoc": "3.3.0-alpha13",
    "jshint": "^2.5.10",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.0.1",
    "mocha-lcov-reporter": "^0.0.1",
    "should": "^4"
  },
  "keywords": [
    "led",
    "control",
    "light",
    "lights",
    "morse",
    "cli",
    "terminal",
    "commandline",
    "leds",
    "raspberry",
    "banana",
    "beaglebone"
  ],
  "jshintConfig": {
    "asi": true,
    "camelcase": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxparams": 4,
    "maxlen": 100,
    "mocha": true,
    "newcap": true,
    "node": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "quotmark": "single",
    "strict": true,
    "undef": true,
    "unused": true
  }
}
